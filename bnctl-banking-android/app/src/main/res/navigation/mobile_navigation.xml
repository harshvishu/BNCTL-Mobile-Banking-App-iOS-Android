            android:label="@string/nav_dashboard"
            tools:layout="@layout/fragment_dashboard">

            <action
                android:id="@+id/action_nav_fragment_dashboard_to_nav_fragment_account_details"
                app:destination="@+id/nav_fragment_account_details" />

            <argument
                android:name="redirectToAccountId"
                android:defaultValue="@null"
                app:argType="string"
                app:nullable="true" />

        </fragment>

        <fragment
            android:id="@+id/nav_fragment_account_details"
            android:name="tl.bnctl.banking.ui.banking.fragments.accounts.details.AccountDetailsFragment"
            tools:layout="@layout/fragment_account_details">

