            tools:layout="@layout/fragment_account_additional_details">

            <argument
                android:name="beneficiary"
                android:defaultValue="beneficiary"
                app:argType="string"
                app:nullable="false" />

            <argument
                android:name="accountNumber"
                android:defaultValue="accountNumber"
                app:argType="string"
                app:nullable="false" />

            <argument
                android:name="bic"
                android:defaultValue="bic"
                app:argType="string"
                app:nullable="false" />

            <argument
                android:name="typeAccount"
                android:defaultValue="typeAccount"
                app:argType="string"
                app:nullable="false" />

            <argument
                android:name="currency"
                android:defaultValue="currency"
                app:argType="string"
                app:nullable="false" />

        </fragment>

